{
  "swagger": "2.0",
  "info": {
    "title": "API V1",
    "version": "v1"
  },
  "paths": {
    "/api/v1/auth": {
      "post": {
        "summary": "Creates a new user",
        "tags": [
          "Registration"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "user",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "email": {
                  "type": "string"
                },
                "password": {
                  "type": "string"
                }
              },
              "required": [
                "email",
                "password",
                "password_confirmation"
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "user registered",
            "examples": {
              "application/json": {
                "status": "success",
                "data": {
                  "id": 1,
                  "provider": "email",
                  "uid": "salvador@dubuqueabernathy.co",
                  "allow_password_change": false,
                  "email": "salvador@dubuqueabernathy.co",
                  "created_at": "2019-07-01T15:02:56.459Z",
                  "updated_at": "2019-07-01T15:02:56.523Z",
                  "role": "user"
                }
              }
            }
          },
          "422": {
            "description": "invalid request",
            "examples": {
              "application/json": {
                "status": "error",
                "data": {
                  "id": null,
                  "provider": "email",
                  "uid": "",
                  "allow_password_change": false,
                  "email": "email",
                  "created_at": null,
                  "updated_at": null,
                  "role": "user"
                },
                "errors": {
                  "password": [
                    "can't be blank"
                  ],
                  "email": [
                    "is not an email",
                    "is invalid"
                  ],
                  "full_messages": [
                    "Password can't be blank",
                    "Email is not an email",
                    "Email is invalid"
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/auth/sign_in": {
      "post": {
        "summary": "Creates a new session",
        "tags": [
          "Session"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "user",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "email": {
                  "type": "string"
                },
                "password": {
                  "type": "string"
                }
              },
              "required": [
                "email",
                "password"
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "session created",
            "examples": {
              "application/json": {
                "data": {
                  "id": 2,
                  "email": "test@gmail.com",
                  "provider": "email",
                  "uid": "test@gmail.com",
                  "allow_password_change": false,
                  "role": "user"
                }
              }
            }
          },
          "401": {
            "description": "unauthorized",
            "examples": {
              "application/json": {
                "success": false,
                "errors": [
                  "Invalid login credentials. Please try again."
                ]
              }
            }
          }
        }
      }
    },
    "/api/v1/auth/sign_out": {
      "delete": {
        "summary": "deleting session",
        "tags": [
          "Session"
        ],
        "consumes": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "session deleted",
            "examples": {
              "application/json": {
                "success": true
              }
            }
          }
        }
      }
    },
    "/api/v1/tasks": {
      "post": {
        "summary": "Creates a new task",
        "tags": [
          "Tasks"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "task",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "description": {
                  "type": "string"
                },
                "notification_date": {
                  "type": "string"
                }
              },
              "required": [
                "name",
                "description"
              ]
            }
          }
        ],
        "responses": {
          "201": {
            "description": "task created",
            "examples": {
              "application/json": {
                "data": {
                  "id": "1",
                  "type": "task",
                  "attributes": {
                    "name": "1111",
                    "description": "rswgwgefwag"
                  },
                  "relationships": {
                    "user": {
                      "data": {
                        "id": "4",
                        "type": "user"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "get": {
        "summary": "Shows tasks",
        "tags": [
          "Tasks"
        ],
        "consumes": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "task created",
            "examples": {
              "application/json": {
                "data": [
                  {
                    "id": "3",
                    "type": "task",
                    "attributes": {
                      "name": "Utapau",
                      "description": "description"
                    },
                    "relationships": {
                      "user": {
                        "data": {
                          "id": "5",
                          "type": "user"
                        }
                      }
                    }
                  },
                  {
                    "id": "2",
                    "type": "task",
                    "attributes": {
                      "name": "Jedha",
                      "description": "description"
                    },
                    "relationships": {
                      "user": {
                        "data": {
                          "id": "5",
                          "type": "user"
                        }
                      }
                    }
                  }
                ]
              }
            }
          }
        }
      }
    },
    "/api/v1/tasks/{id}": {
      "get": {
        "summary": "Show a task",
        "tags": [
          "Tasks"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "type": "string",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "Task founded",
            "examples": {
              "application/json": {
                "data": {
                  "id": "4",
                  "type": "task",
                  "attributes": {
                    "name": "Dagobah",
                    "description": "description"
                  },
                  "relationships": {
                    "user": {
                      "data": {
                        "id": "6",
                        "type": "user"
                      }
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Task not found"
          }
        }
      },
      "delete": {
        "summary": "Delete a task",
        "tags": [
          "Tasks"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "type": "string",
            "required": true
          }
        ],
        "responses": {
          "204": {
            "description": "Task destroyed"
          }
        }
      },
      "put": {
        "summary": "Update a task",
        "tags": [
          "Tasks"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "type": "string",
            "required": true
          },
          {
            "name": "task",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "description": {
                  "type": "string"
                },
                "notification_date": {
                  "type": "string"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "task created",
            "examples": {
              "application/json": {
                "data": {
                  "id": "6",
                  "type": "task",
                  "attributes": {
                    "name": "Hosnian Prime",
                    "description": "description"
                  },
                  "relationships": {
                    "user": {
                      "data": {
                        "id": "9",
                        "type": "user"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
