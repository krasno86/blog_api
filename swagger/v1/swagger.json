{
  "swagger": "2.0",
  "info": {
    "title": "API V1",
    "version": "v1"
  },
  "paths": {
    "/api/v1/articles": {
      "post": {
        "summary": "Creates a new article",
        "tags": [
          "Articles"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "article",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "description": {
                  "type": "string"
                },
                "avatar": {
                  "type": "string"
                }
              },
              "required": [
                "name",
                "description"
              ]
            }
          }
        ],
        "responses": {
          "201": {
            "description": "article created",
            "examples": {
              "application/json": {
                "data": {
                  "id": "1",
                  "type": "article",
                  "attributes": {
                    "name": "1111",
                    "description": "rswgwgefwag"
                  },
                  "relationships": {
                    "user": {
                      "data": {
                        "id": "1",
                        "type": "user"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "get": {
        "summary": "Shows articles",
        "tags": [
          "Articles"
        ],
        "consumes": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "Articles founded",
            "examples": {
              "application/json": {
                "data": [
                  {
                    "id": "3",
                    "type": "article",
                    "attributes": {
                      "name": "Alderaan",
                      "description": "description"
                    },
                    "relationships": {
                      "user": {
                        "data": {
                          "id": "2",
                          "type": "user"
                        }
                      }
                    }
                  },
                  {
                    "id": "2",
                    "type": "article",
                    "attributes": {
                      "name": "Ahch-To",
                      "description": "description"
                    },
                    "relationships": {
                      "user": {
                        "data": {
                          "id": "2",
                          "type": "user"
                        }
                      }
                    }
                  }
                ]
              }
            }
          }
        }
      }
    },
    "/api/v1/articles/{id}": {
      "get": {
        "summary": "Show article",
        "tags": [
          "Articles"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "type": "string",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "Article found",
            "examples": {
              "application/json": {
                "data": {
                  "id": "4",
                  "type": "article",
                  "attributes": {
                    "name": "Ahch-To",
                    "description": "description"
                  },
                  "relationships": {
                    "user": {
                      "data": {
                        "id": "3",
                        "type": "user"
                      }
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Article not found"
          }
        }
      },
      "delete": {
        "summary": "Delete article",
        "tags": [
          "Articles"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "type": "string",
            "required": true
          }
        ],
        "responses": {
          "204": {
            "description": "Articles destroyed"
          }
        }
      },
      "put": {
        "summary": "Update article",
        "tags": [
          "Articles"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "type": "string",
            "required": true
          },
          {
            "name": "article",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "description": {
                  "type": "string"
                },
                "avatar": {
                  "type": "string"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "group created",
            "examples": {
              "application/json": {
                "data": {
                  "id": "6",
                  "type": "article",
                  "attributes": {
                    "name": "Crait",
                    "description": "description"
                  },
                  "relationships": {
                    "user": {
                      "data": {
                        "id": "6",
                        "type": "user"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/auth": {
      "post": {
        "summary": "Creates a new user",
        "tags": [
          "Registration"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "user",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "email": {
                  "type": "string"
                },
                "password": {
                  "type": "string"
                }
              },
              "required": [
                "email",
                "password",
                "password_confirmation"
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "user registered",
            "examples": {
              "application/json": {
                "status": "success",
                "data": {
                  "id": 7,
                  "provider": "email",
                  "uid": "herschelbednar@schiller.co",
                  "allow_password_change": false,
                  "email": "herschelbednar@schiller.co",
                  "created_at": "2019-09-26T14:09:35.963Z",
                  "updated_at": "2019-09-26T14:09:36.027Z"
                }
              }
            }
          },
          "422": {
            "description": "invalid request",
            "examples": {
              "application/json": {
                "status": "error",
                "data": {
                  "id": null,
                  "provider": "email",
                  "uid": "",
                  "allow_password_change": false,
                  "email": "email",
                  "created_at": null,
                  "updated_at": null
                },
                "errors": {
                  "password": [
                    "can't be blank"
                  ],
                  "email": [
                    "is not an email",
                    "is invalid"
                  ],
                  "full_messages": [
                    "Password can't be blank",
                    "Email is not an email",
                    "Email is invalid"
                  ]
                }
              }
            }
          }
        }
      },
      "delete": {
        "summary": "deleting user",
        "tags": [
          "Registration"
        ],
        "consumes": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "user deleted",
            "examples": {
              "application/json": {
                "status": "success",
                "message": "Account with UID 'test@example.com' has been destroyed."
              }
            }
          }
        }
      }
    },
    "/auth/sign_in": {
      "post": {
        "summary": "Creates a new session",
        "tags": [
          "Session"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "user",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "email": {
                  "type": "string"
                },
                "password": {
                  "type": "string"
                }
              },
              "required": [
                "email",
                "password"
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "session created",
            "examples": {
              "application/json": {
                "data": {
                  "id": 9,
                  "email": "test@example.com",
                  "provider": "email",
                  "uid": "test@example.com",
                  "allow_password_change": false
                }
              }
            }
          },
          "401": {
            "description": "unauthorized",
            "examples": {
              "application/json": {
                "success": false,
                "errors": [
                  "Invalid login credentials. Please try again."
                ]
              }
            }
          }
        }
      }
    },
    "/auth/sign_out": {
      "delete": {
        "summary": "deleting session",
        "tags": [
          "Session"
        ],
        "consumes": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "session deleted",
            "examples": {
              "application/json": {
                "success": true
              }
            }
          }
        }
      }
    }
  },
  "securityDefinitions": {
    "accessToken": {
      "type": "apiKey",
      "name": "access-token",
      "in": "header"
    },
    "tokenType": {
      "type": "apiKey",
      "name": "token-type",
      "in": "header"
    },
    "client": {
      "type": "apiKey",
      "name": "client",
      "in": "header"
    },
    "expiry": {
      "type": "apiKey",
      "name": "expiry",
      "in": "header"
    },
    "uid": {
      "type": "apiKey",
      "name": "uid",
      "in": "header"
    }
  },
